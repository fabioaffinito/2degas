# make defs for Galileo
F90=mpifc
BLAS_INTERNAL=
#
# other fpp flags include -DDEBUG
#FPP=-fpp -DBLAS_INTERNAL -DDEBUG
FPP=-fpp
F90FLAGS= -O3 -g -mmic -qopt-report-annotated

LDFLAGS=-mmic
ifdef BLAS_INTERNAL
   EXE=rept90-int-mic.x
   FPP:= $(FPP) -DBLAS_INTERNAL
else
   EXE=rept90-mkl-mic.x
   F90_OBJS:= tools.o $(F90_OBJS)
   LDFLAGS:=-mkl
endif

