# make defs for PICO
F90=mpif90
BLAS_INTERNAL=
#
# other fpp flags include -DDEBUG
#FPP=-fpp -DBLAS_INTERNAL -DDEBUG
FPP=-fpp -DDEBUG
F90FLAGS= -O3 -g 

ifdef BLAS_INTERNAL
   EXE=rept90-int.x
   FPP:= $(FPP) -DBLAS_INTERNAL
   LDFLAGS=
else
   EXE=rept90-mkl.exe
   F90_OBJS:= tools.o $(F90_OBJS)
   LDFLAGS=-mkl
endif

