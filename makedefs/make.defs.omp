# make defs for OMP Galileo
#
F90=ifort
BLAS_INTERNAL=
EXE=rept90-omp.x
#
# other fpp flags include -DDEBUG
#FPP=-fpp -DBLAS_INTERNAL -DDEBUG
FPP=-fpp

F90FLAGS= -O0 -g -qopenmp

ifdef BLAS_INTERNAL
   EXE=rept90-int.x
   FPP:= $(FPP) -DBLAS_INTERNAL
   LDFLAGS=
else
   F90_OBJS:= tools.o $(F90_OBJS)
   LDFLAGS=-mkl
endif

